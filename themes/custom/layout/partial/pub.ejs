<div class="pub mb-3">
    <!-- title -->
    <div class="pub-title">
        <a href="<%= pub.url ? pub.url : '' %>">
            <b><%- pub.title %></b>
        </a>
    </div>

    <!-- authors -->
    <% for (let [idx, author] of pub.authors.entries()) { %>
        <% if (author.includes("Jaeyoon Song")) { %>
            <i><%- author %></i><% if (idx !== pub.authors.length - 1) { %>, <% } %>
        <% } else { %>
            <%- author %><% if (idx !== pub.authors.length - 1) { %>, <% } %>
        <% } %>
    <% } %>

    <div>
        <% if (pub.equal_contribution) { %>
            <small><i>* Equal contribution</i></small>
        <% } %>
    </div>

    <!-- venue -->
    <% if (pub.conference) { %>
        <%- pub.conference.title %> <%- pub.year %><% if (pub.upcoming) { %>, to appear<% } %>
    <% } else if (pub.journal) { %>
        <%- pub.journal.title %> vol. <%- pub.journal.volume %>, no. <%- pub.journal.issue %>, pp. <%- pub.journal.page %>, <%- pub.year %><% if (pub.upcoming) { %>, to appear<% } %>
    <% } %>

    <!-- award -->
    <% if (pub.award) { %>
        <div class="text-muted">
            <small>
                üèÜ Won <%- pub.award %> üèÜ
            </small>
        </div>
    <% } %>

    <!-- links -->
    <% const project = site.data.projects.filter(project => project.id === pub.id) %>
    <% if (project.length > 0) { %>
        <%- partial("partial/links", {project: project}) %>
    <% } %>
    

</div>